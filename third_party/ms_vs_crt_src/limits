		{	// return most negative value
		return ((min)());
		}
 #endif /* _HAS_CPP0X */

	static _Ty __CRTDECL epsilon() _THROW0()
		{	// return smallest effective increment from 1.0
		return (0);
		}

	static _Ty __CRTDECL round_error() _THROW0()
		{	// return largest rounding error
		return (0);
		}

