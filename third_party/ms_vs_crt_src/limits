		// CLASS numeric_limits<long>
template<> class _CRTIMP2_PURE numeric_limits<long>
	: public _Num_int_base
	{	// limits for type long
public:
	typedef long _Ty;

	static _Ty (__CRTDECL min)() _THROW0()
		{	// return minimum value
		return (LONG_MIN);
		}

