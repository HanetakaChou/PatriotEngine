{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/obj/local/x86_64/vkcube", //should set a.out in obj here instead of in libs
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "miDebuggerServerAddress": ":27177",
            "MIMode": "gdb",
            "miDebuggerPath": "${env:HOME}/.local/libexec/android-ndk-r11c/prebuilt/linux-x86_64/bin/gdb",
            "miDebuggerArgs": "--init-command=${workspaceFolder}/libs/x86_64/gdb.setup",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": false
                }
            ]
        }
    ]
}